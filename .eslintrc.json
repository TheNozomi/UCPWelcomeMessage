{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "consistent-return": ["off"],
        "curly": ["error", "multi-or-nest"],
        "max-len": ["error", { "code": 140 }],
        "no-console": ["off"],
        "no-underscore-dangle": ["off"],
        "one-var": ["off"]
    }
}